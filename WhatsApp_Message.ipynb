    "import pywhatkit\n",
    "import time\n",
    "\n",
    "\n",
    "contacts = {\n",
    "    \"+91XXXXXXXXXXXX\": \"Hello,what are you doing\",\n",
    "}\n",
    "\n",
    "for number, msg in contacts.items():\n",
    "    print(f\"Sending message to {number}...\")\n",
    "    pywhatkit.sendwhatmsg_instantly(number, msg)\n",
    "    time.sleep(5) "
